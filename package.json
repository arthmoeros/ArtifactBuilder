{
  "name": "@artifacter/ui",
  "version": "1.0.0",
  "description": "UI generation tool for Artifacter",
  "scripts": {
    "ui-builder": "tsc -p ./ui-builder/src && node ./ui-builder/src/ui-builder-main.js",
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "start": "node index.js",
    "dev": "npm run ui-builder && concurrent \"npm run build:watch\" \"node index.js\""
  },
